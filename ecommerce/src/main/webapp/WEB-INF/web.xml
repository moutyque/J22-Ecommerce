<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0">
	<jsp-config>
		<jsp-property-group>
			<url-pattern>*.jsp</url-pattern>
			<include-prelude>/WEB-INF/taglibs.jsp</include-prelude>
		</jsp-property-group>
	</jsp-config>
	<display-name>Archetype Created Web Application</display-name>

	<!-- Client servlets -->
	<servlet>
		<servlet-name>clientCreation</servlet-name>
		<servlet-class>com.ecommerce.servlets.CreationClient</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>


	<!-- Commande servlets -->
	<servlet>
		<servlet-name>commandeCreation</servlet-name>
		<servlet-class>com.ecommerce.servlets.CreationCommande</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>


	<!-- Client servlets mapping -->
	<servlet-mapping>
		<servlet-name>clientCreation</servlet-name>
		<url-pattern>/clientCreation</url-pattern>
	</servlet-mapping>


	<!-- Commande servlets mapping -->
	<servlet-mapping>
		<servlet-name>commandeCreation</servlet-name>
		<url-pattern>/commandeCreation</url-pattern>
	</servlet-mapping>


</web-app>
